{
  "name": "dom-find-and-replace",
  "version": "1.1.0",
  "description": "Find some text in the dom and replace with what you want",
  "main": "dist/index.js",
  "author": "Youngjin Jang",
  "repository": {
    "type": "git",
    "url": "https://github.com/swimyoung/dom-find-and-replace"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "report": "codecov",
    "lint": "eslint .",
    "dev": "webpack-dev-server --config ./docs/webpack.config.js",
    "docs": "cross-env NODE_ENV=production webpack -p --config ./docs/webpack.config.js",
    "compile": "cross-env NODE_ENV=production babel src -d dist --ignore **/*.spec.js",
    "bundle": "cross-env NODE_ENV=production webpack -p",
    "dist": "rimraf dist && run-p compile bundle docs"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-plugin-lodash": "^3.3.4",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.5.0",
    "jest-in-case": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "chance": "^1.0.18",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.5.4",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "styled-components": "^4.1.3",
    "@babel/preset-react": "^7.0.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "lodash": "^4.17.11"
  },
  "sideEffects": false,
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "all"
  },
  "jest": {
    "testURL": "http://localhost",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
