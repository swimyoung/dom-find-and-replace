{
  "name": "dom-find-and-replace",
  "version": "1.4.6",
  "description": "Find some text in the dom and replace with what you want",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "author": "Youngjin Jang",
  "repository": {
    "type": "git",
    "url": "https://github.com/swimyoung/dom-find-and-replace"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "report": "codecov",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint --ext .ts .",
    "tsc:dry": "tsc --noEmit --esModuleInterop",
    "transpile:esm": "tsc -p tsconfig.json",
    "transpile:cjs": "tsc -p tsconfig.cjs.json",
    "dist": "rimraf dist && run-p transpile:*",
    "demo:dev": "vite",
    "demo:build": "rimraf dist-demo && vite build",
    "demo:preview": "vite preview"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/jest": "29.2.5",
    "@types/jest-in-case": "1.0.6",
    "@types/node": "18.11.18",
    "@types/randomcolor": "0.5.7",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "chance": "1.1.9",
    "codecov": "3.8.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.3",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-in-case": "1.0.2",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.2",
    "randomcolor": "0.6.2",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4",
    "vite": "4.0.4"
  },
  "dependencies": {
    "dom-node-type": "1.1.1"
  }
}
