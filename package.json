{
  "name": "dom-find-and-replace",
  "version": "1.4.5",
  "description": "Find some text in the dom and replace with what you want",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "author": "Youngjin Jang",
  "repository": {
    "type": "git",
    "url": "https://github.com/swimyoung/dom-find-and-replace"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "report": "codecov",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint --ext .ts .",
    "tsc:dry": "tsc --noEmit --esModuleInterop",
    "transpile:esm": "tsc -p tsconfig.json",
    "transpile:cjs": "tsc -p tsconfig.cjs.json",
    "dist": "rimraf dist && run-p transpile:*"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/jest-in-case": "1.0.3",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "codecov": "3.8.1",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest": "24.3.3",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "jest-in-case": "1.0.2",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "dom-node-type": "1.1.1"
  }
}
